package day0315;

public class Ex10_Operator 
{
	public static void main(String[] args) 
	{
		//증감연산자
		//전치일 경우 1순위
		//후치일 경우 끝순위
		//단항일 경우 상관 없음
		
		int m = 5, n = 5, a = 10, b = 10;
		m++;//단항이라 상관 없음
		++n;//단항이라 상관 없음
		System.out.println("m = " + m);//6
		System.out.println("n = " + n);//6
		
		a += m++;//a = a + m, m + 1;
		System.out.println("a = " + a);
		System.out.println("m = " + m);
		
		
		
	}

}
