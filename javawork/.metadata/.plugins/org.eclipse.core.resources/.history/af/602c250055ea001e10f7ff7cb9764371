package day0325;

public class Employee 
{
	String name;
	String position;
	int age;
	int gibon;
	int sudang;
	
	//생성자
	public Employee() {
		super();
	}
	public Employee(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	public Employee(String name, String position, int age) {
		super();
		this.name = name;
		this.position = position;
		this.age = age;
	}
	
	
	
	
	
	//getter & setter
	
	
}
