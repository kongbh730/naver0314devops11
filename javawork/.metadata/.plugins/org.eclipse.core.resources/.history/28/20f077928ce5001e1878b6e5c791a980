package day0319;

public class Ex2_Factorial {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int i, j;
		int factorial;
		
		for(i = 1; i <= 10; i++)
		{
			factorial = 1;
			for(j= 1; j <= i; j++)
			{
				factorial *= j;
			}
			System.out.printf("%d! = %d\n", factorial);
		}
	}

}
