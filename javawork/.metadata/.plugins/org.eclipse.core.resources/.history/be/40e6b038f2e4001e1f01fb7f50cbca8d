package day0318;

public class Ex3_Multiple_for {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i, j;
		
		for(i = 1; j < 5; i++)
		{
			System.out.println("Hello");
			for(j = 1; j <= 3; j++)
			{
				System.out.printf("\tkitty");
			}
			System.out.println();
		}
	}

}
