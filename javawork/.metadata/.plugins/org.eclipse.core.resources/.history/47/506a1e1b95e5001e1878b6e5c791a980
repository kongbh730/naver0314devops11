package day0319;

public class Ex1_FindingPrimeNum {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int count = 0;
		int i, j;
		boolean prime = true;
		
		//1은 소수가 아닌데....
		for(i = 1; i <= 100; i++)
		{
			for(j = 2; j*j <= i; j++)
			{
				if(i % j == 0)
				{
					//소수가 아님
					prime = false;
					break;
				}
				else
				{
					prime = true;
				}
			}
			if(prime)
			{
				System.out.println(i);
				count++;
			}
		}
		System.out.println("소수의 개수 : " + count);
	}
}
