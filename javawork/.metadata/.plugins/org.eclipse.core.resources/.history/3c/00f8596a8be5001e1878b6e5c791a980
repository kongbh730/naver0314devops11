package day0319;

public class Ex1_MultipleFor {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int count = 0;
		int i, j;
		
		for(i = 1; i <= 100; i++)
		{
			for(j = 2; j*j <= i; j++)
			{
				if(i%j == 0)
				{
					break;
				}
				System.out.println(i);
				count++;
				break;
			}
		}
		System.out.println("소수의 개수 : " + count);
	}

}
